{
  "name": "otus-vue-lab5",
  "version": "0.0.5",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "test": "npm run test:unit -- --run && CI=1 npm run test:e2e -- -x",
    "test:unit": "vitest",
    "coverage:unit": "npm run test:unit -- --coverage",
    "test:e2e": "playwright test",
    "report:e2e": "playwright show-report",
    "codegen:e2e": "playwright codegen localhost:5173",
    "build-only": "vite build",
    "build-only:gh-pages": "vite build --base=/otus-vue-lab5",
    "test:unit:gh-pages": "CI=1 vitest --run",
    "test:e2e:gh-pages": "vite build && playwright install --with-deps && CI=1 playwright test -x"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "pinia": "^2.0.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.1.3",
    "veaury": "^2.3.11",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@playwright/test": "^1.28.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vitest/ui": "^0.28.5",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.7.4",
    "vite": "^4.0.0",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.12"
  }
}
